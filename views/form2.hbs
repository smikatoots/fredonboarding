<script>
window.onload = () => {
    $(document).ready(() => {
        $('#form2').hide();
        $('#checkbox').change(function()
        {
          if ($(this).is(':checked')) {
            checked = true;
            $('#form2').show('');
            $('#form2-none').hide();
          } else {
            checked = false;
            $('#form2').hide();
            $('#form2-none').show();
          };
        });
    })
}
</script>

