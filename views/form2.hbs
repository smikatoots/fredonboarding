<script>
window.onload = () => {
  $(document).ready(() => {
      $('.yes-blank').hide();
      var fieldsArray = ['#secondaryOfficerListed',
      '#secondaryOfficerBroker',
      '#secondaryShareholderBroker',
      '#secondaryExistingAccount',
      '#secondaryAnotherAccount']
      console.log(fieldsArray);
      for (var i = 0; i < fieldsArray.length; i++) {
          console.log(i, fieldsArray[i]);
          if ($(fieldsArray[i]).siblings('.yes-blank').val() !== "") {
              $(fieldsArray[i]).attr('checked', 'checked');
              $(fieldsArray[i]).siblings('.yes-blank').show()
          }
          $(fieldsArray[i]).change(function()
          {
            if ($(this).is(':checked')) {
                $(this).siblings('.yes-blank').show()
            } else {
              $(this).siblings('.yes-blank').val("")
              $(this).siblings('.yes-blank').hide()
            };
          });
      }
  })
}
</script>

